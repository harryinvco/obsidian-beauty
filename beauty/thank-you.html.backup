        // 48-hour countdown timer synced with main page
        function initOfferCountdown() {
            // 2 days in ms
            const duration = 48 * 60 * 60 * 1000;
            // Store/retrieve end time in localStorage for persistence
            let endTime = localStorage.getItem('campaignEndTime');
            if (!endTime || Number(endTime) < Date.now()) {
                endTime = Date.now() + duration;
                localStorage.setItem('campaignEndTime', endTime);
            }

            function updateTimer() {
                let timeLeft = endTime - Date.now();
                if (timeLeft <= 0) {
                    // Restart timer for another 2 days
                    endTime = Date.now() + duration;
                    localStorage.setItem('campaignEndTime', endTime);
                    timeLeft = endTime - Date.now();
                }

                const hours = Math.floor(timeLeft / (1000 * 60 * 60));
                const minutes = Math.floor((timeLeft % (1000 * 60 * 60)) / (1000 * 60));
                const seconds = Math.floor((timeLeft % (1000 * 60)) / 1000);
                const timerDisplay = `${hours.toString().padStart(2, '0')}:${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;

                const timerElement = document.getElementById('offer-timer');
                if (timerElement) {
                    timerElement.textContent = timerDisplay;
                }
            }

            // Update immediately and then every second
            updateTimer();
            const timerInterval = setInterval(updateTimer, 1000);
        }
        
        // Get email from localStorage or URL and display it in the confirmation heading
        function displayUserEmail() {
            let email = null;

            // Try localStorage first
            try {
                const leadData = localStorage.getItem('leadData');
                if (leadData) {
                    const data = JSON.parse(leadData);
                    if (data && data.email) email = data.email;
                }
            } catch (e) {
                console.warn('Could not read leadData from localStorage', e);
            }

            // Fallback to URL param ?email= if present
            if (!email) {
                try {
                    const params = new URLSearchParams(window.location.search);
                    const urlEmail = params.get('email');
                    if (urlEmail) email = decodeURIComponent(urlEmail);
                } catch (e) {
                    // ignore
                }
            }

            const emailSpan = document.getElementById('user-email');
            if (emailSpan && email) {
                emailSpan.textContent = email;
                emailSpan.style.color = '#fbbf24';
            } else if (emailSpan && !email) {
                // If no email found, show a generic message
                emailSpan.textContent = 'your inbox';
            }
        }
        
        // Initialize everything when page loads
        document.addEventListener('DOMContentLoaded', function() {
            initOfferCountdown();
            displayUserEmail();
        });
        
        // Track page view for analytics
        if (typeof gtag !== 'undefined') {
            gtag('event', 'page_view', {
                page_title: 'Thank You Page - Upsell',
                page_location: window.location.href
            });
            
            gtag('event', 'conversion', {
                send_to: 'AW-CONVERSION_ID/THANK_YOU_CONVERSION',
                value: 997,
                currency: 'USD'
            });
        }
        
        // Update URL parameters for social sharing
        document.addEventListener('DOMContentLoaded', function() {
            const currentUrl = encodeURIComponent(window.location.origin);
            
            // Update social share links if they exist
            const twitterLink = document.querySelector('a[href*="twitter.com"]');
            if (twitterLink) {
                twitterLink.href = twitterLink.href + currentUrl;
            }
            
            const facebookLink = document.querySelector('a[href*="facebook.com"]');
            if (facebookLink) {
                facebookLink.href = facebookLink.href + currentUrl;
            }
            
            const linkedinLink = document.querySelector('a[href*="linkedin.com"]');
            if (linkedinLink) {
                linkedinLink.href = linkedinLink.href + currentUrl;
            }
        });
    </script>
</body>
</html>